<%= form_with(model: note) do |form|%>
    <div>
        <%= form.label :title%>
        <%= form.text_field :title, style: "display: block"%>
    </div>
    <div>
        <%= form.label :content%>
        <%= form.rich_text_area :content, style: "display: block"%>
    </div>
    <div>
        <%#= form.hidden_field :user_id, value: current_user.id %>
    </div>
    <div>
        <%= form.submit %>
    </div>
<% end %>