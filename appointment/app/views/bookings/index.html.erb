<h2> List of Bookings </h2>
<%= link_to "New Booking", new_booking_path%>
<div>
<% @bookings.each do |booking| %>
<p> Start:
    <%= booking.start %>
</p>
<p> End:
    <%= booking.end %>
</p>
<p> Purpose:
    <%= booking.purpose %>
</p>
<p> Room:
    <%= link_to booking.room.name, booking.room%>
</p>
<% end %>
</div>